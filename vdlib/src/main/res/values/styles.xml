<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="vdPager">
		<attr name="animateChanges" format="boolean"/>
		<attr name="striptype" format="enum">
			<enum name="normal" value="0"/>
			<enum name="tab" value="1" />
		</attr>
	</declare-styleable>

	<declare-styleable name="SplitView">
		<attr name="percentage" format="integer"/>
		<attr name="textBackColor" format="color"/>
		<attr name="radius" format="dimension"/>
		<attr name="border" format="dimension"/>
		<attr name="borderColor" format="color"/>
	</declare-styleable>

	<declare-styleable name="SweetButton">
		<attr name="text" format="string"/>
		<attr name="src" format="integer"/>
		<attr name="size" format="dimension"/>
		<attr name="textPadding" format="dimension"/>
		<attr name="textColor" format="color"/>
		<attr name="textBack" format="color"/>
		<attr name="imgBack" format="color"/>
	</declare-styleable>

</resources>
